<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="PendingAction_OTPSuite">

  <listeners>
        <listener class-name="docuexec.EnduserAllModules.MyTestListener"/>
    </listeners>
      <test name="O_Finish_CurrentPage">
        <classes>
<class name="docuexec.EnduserAllModules.PendingAction">
                <methods>
                    <include name="O_Finish_PendingActionOnCurrentPage"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="O_Finish_AllPages">
        <classes>
            <class name="docuexec.EnduserAllModules.PendingAction">
                <methods>
                    <include name="O_Finish_PendingActionOnAllPages"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="O_Finish_CustomPages">
        <classes>
            <class name="docuexec.EnduserAllModules.PendingAction">
                <methods>
                    <include name="O_Finish_PendingActionOnCustomPages"/>
