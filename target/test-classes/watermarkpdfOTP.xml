<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Test Otp signing">
    <listeners>
        <listener class-name="docuexec.EnduserAllModules.MyTestListener"/>
    </listeners>

    <test thread-count="1" name="Test_OTPFinish_OTestcurrentPage">
        <classes>
            <class name="docuexec.EnduserAllModules.Watermarkpdf">
                <methods>
                    <include name="OTestcurrentPage"/>
                    <include name="close"/>
                </methods>
            </class>
        </classes>
    </test>

